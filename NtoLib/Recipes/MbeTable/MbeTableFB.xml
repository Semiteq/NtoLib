					<Pin ID="206" Name="Shutter6"	Type="Строковый"/>
					<Pin ID="207" Name="Shutter7"	Type="Строковый"/>
					<Pin ID="208" Name="Shutter8"	Type="Строковый"/>
					<Pin ID="209" Name="Shutter9"	Type="Строковый"/>
					<Pin ID="210" Name="Shutter10" Type="Строковый"/>
					<Pin ID="211" Name="Shutter11" Type="Строковый"/>
					<Pin ID="212" Name="Shutter12" Type="Строковый"/>
					<Pin ID="213" Name="Shutter13" Type="Строковый"/>
					<Pin ID="214" Name="Shutter14" Type="Строковый"/>
					<Pin ID="215" Name="Shutter15" Type="Строковый"/>
					<Pin ID="216" Name="Shutter16" Type="Строковый"/>
					<Pin ID="217" Name="Shutter17" Type="Строковый"/>
					<Pin ID="218" Name="Shutter18" Type="Строковый"/>
					<Pin ID="219" Name="Shutter19" Type="Строковый"/>
					<Pin ID="220" Name="Shutter20" Type="Строковый"/>
					<Pin ID="221" Name="Shutter21" Type="Строковый"/>
					<Pin ID="222" Name="Shutter22" Type="Строковый"/>
					<Pin ID="223" Name="Shutter23" Type="Строковый"/>
					<Pin ID="224" Name="Shutter24" Type="Строковый"/>
					<Pin ID="225" Name="Shutter25" Type="Строковый"/>
					<Pin ID="226" Name="Shutter26" Type="Строковый"/>
					<Pin ID="227" Name="Shutter27" Type="Строковый"/>
					<Pin ID="228" Name="Shutter28" Type="Строковый"/>
					<Pin ID="229" Name="Shutter29" Type="Строковый"/>
					<Pin ID="230" Name="Shutter30" Type="Строковый"/>
					<Pin ID="231" Name="Shutter31" Type="Строковый"/>
					<Pin ID="232" Name="Shutter32" Type="Строковый"/>
				</Items>
			</Group>

			<!-- Group of heater names -->
			<Group Name="HeaterNames" ID="300">
				<Items>
					<Pin ID="301" Name="Heater1"	Type="Строковый" DefaultValue="He1"/>
					<Pin ID="302" Name="Heater2"	Type="Строковый" DefaultValue="He2"/>
					<Pin ID="303" Name="Heater3"	Type="Строковый"/>
					<Pin ID="304" Name="Heater4"	Type="Строковый"/>
					<Pin ID="305" Name="Heater5"	Type="Строковый"/>
