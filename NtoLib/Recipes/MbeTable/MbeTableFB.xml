<?xml version="1.0" encoding="utf-8" ?>
<FBConfig>

	<Map>
		<!-- Non visual pins start with 1 -->
		<Items>
			<!-- Flag if recipe is curently running.  -->
			<Pin ID="1" Name="RecipeActive"			Type="Логический" DefaultValue="false"/>
			
			<!-- Flag if recipe is paused.  -->
			<Pin ID="2" Name="RecipePaused"			Type="Логический" DefaultValue="false"/>
			
			<!-- Current recipe line number in opertation mode. -->
			<Pin ID="3" Name="ActualLineNumber"		Type="Целый"/>

			<!-- Current recipe execution time.  -->
			<Pin ID="4" Name="StepCurrentTime"		Type="Вещественный"/>

			<!-- Number of the current FOR loop by the nesting level.  -->
			<Pin ID="5" Name="ForLoopCount1"		Type="Целый"/>
			<Pin ID="6" Name="ForLoopCount2"		Type="Целый"/>
			<Pin ID="7" Name="ForLoopCount3"		Type="Целый"/>

			<!-- Permission to open new recipe. -->
			<Pin ID="8" Name="EnaLoad"				Type="Логический" DefaultValue="true"/>

			<!-- Group of shutter names -->
			<Group Name="ShutterNames" ID="200">
				<Items>
					<Pin ID="201" Name="Shutter1"	Type="Строковый" DefaultValue="Sh1"/>
					<Pin ID="202" Name="Shutter2"	Type="Строковый" DefaultValue="Sh2"/>
					<Pin ID="203" Name="Shutter3"	Type="Строковый"/>
